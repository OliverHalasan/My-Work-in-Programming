<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RazorPages</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/holiday.css@0.9.8" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/RazorPages.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav>
                <ul>
                    <li>
                        <a asp-area="" asp-page="/Index">Home</a>
                     </li>
                     
                     <li>
                         <span>Exercise</span>
                         <ul>
                             <li><a asp-area ="" asp-page="/ExercisePages/Query">Query</a></li>
                         </ul>

                     </li>

                     <li >
                        <a asp-area="" asp-page="/Privacy">Privacy</a>
                    </li>
                     <partial name="_LoginPartial" />
               </ul>
                   
        </nav>
    </header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div>
            &copy; 2022 - RazorPages - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>